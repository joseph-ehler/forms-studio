{
  "name": "@intstudio/ds",
  "version": "0.3.0",
  "description": "Intelligence Studio Design System - Route shells, hooks, and design tokens",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "design-system",
    "react",
    "components",
    "tokens",
    "primitives",
    "accessibility",
    "a11y",
    "flat-design",
    "white-label",
    "storybook"
  ],
  "author": "Intelligence Studio",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/joseph-ehler/intelligence-studio.git",
    "directory": "packages/wizard-react"
  },
  "homepage": "https://github.com/joseph-ehler/intelligence-studio#readme",
  "bugs": {
    "url": "https://github.com/joseph-ehler/intelligence-studio/issues"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.9.3"
  }
}
