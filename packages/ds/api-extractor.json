{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "./dist/index.d.ts",
  "projectFolder": ".",
  "compiler": {
    "tsconfigFilePath": "./tsconfig.json"
  },
  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "./dist/index.api.d.ts"
  },
  "apiReport": {
    "enabled": true,
    "reportFileName": "ds.api.md",
    "reportFolder": "./etc"
  },
  "docModel": {
    "enabled": false
  },
  "tsdocMetadata": {
    "enabled": false
  },
  "messages": {
    "extractorMessageReporting": {
      "ae-missing-release-tag": {
        "logLevel": "none"
      }
    }
  }
}
