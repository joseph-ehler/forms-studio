{
  "name": "@joseph-ehler/wizard-react",
  "version": "0.2.1",
  "description": "React runtime for Forms Studio: render any JSON field type with RHF + Zod",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "forms",
    "wizard",
    "react",
    "react-hook-form",
    "field-registry",
    "json-forms",
    "dynamic-forms"
  ],
  "author": "Intelligence Studio",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/joseph-ehler/forms-studio.git",
    "directory": "packages/wizard-react"
  },
  "homepage": "https://github.com/joseph-ehler/forms-studio#readme",
  "bugs": {
    "url": "https://github.com/joseph-ehler/forms-studio/issues"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.0.0",
    "zod": "^3.22.0"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@joseph.ehler/wizard-core": "workspace:*",
    "date-fns": "^4.1.0",
    "react-colorful": "^5.6.1",
    "react-day-picker": "^9.11.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "zod": "^3.22.0"
  }
}
