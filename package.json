{
  "name": "intelligence-studio-forms",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "pnpm -r --filter './packages/**' build",
    "dev": "pnpm -r --parallel --filter './packages/**' dev",
    "typecheck": "pnpm -r --filter './packages/**' typecheck",
    "play": "pnpm --filter playground dev",
    "clean": "rm -rf **/dist **/node_modules",
    "validate:docs": "node scripts/validate-docs.mjs",
    "validate:naming": "node scripts/validate-naming.mjs",
    "validate:all": "pnpm validate:docs && pnpm validate:naming",
    "lint:md": "markdownlint \"**/*.md\" --ignore node_modules",
    "test:validator": "node scripts/__tests__/validate-docs.test.mjs",
    "setup:hooks": "node scripts/setup-hooks.mjs",
    "postinstall": "node scripts/setup-hooks.mjs",
    "doc:new": "node scripts/doc-new.mjs"
  },
  "devDependencies": {
    "markdownlint-cli": "^0.45.0",
    "typescript": "^5.9.3"
  }
}
